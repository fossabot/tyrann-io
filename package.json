{
  "version": "1.0.0-alpha.0",
  "license": "MIT",
  "main": "dist/index.cjs",
  "typings": "dist/src/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.mjs"
  },
  "module": "./dist/index.mjs",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle",
    "test": "tsdx test",
    "lint": "prettier --check src/",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "name": "tyrann-io",
  "author": "hanayashiki",
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.cjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/express": "^4.17.11",
    "express": "^4.17.1",
    "husky": "^6.0.0",
    "microbundle": "^0.14.2",
    "morgan": "^1.10.0",
    "prettier": "^2.4.1",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/string-format": "^2.0.0",
    "query-string": "^7.0.0",
    "string-format": "^2.0.0"
  },
  "resolutions": {
    "**/typescript": "^4.2.4"
  }
}
